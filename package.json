{
  "name": "styled-minimal",
  "version": "0.1.1",
  "description": "Minimal UI theme with styled-components",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/gilbarbara/styled-minimal.git"
  },
  "bugs": {
    "url": "https://github.com/gilbarbara/styled-minimal/issues"
  },
  "homepage": "https://github.com/gilbarbara/styled-minimal#readme",
  "main": "index.js",
  "module": "index.es.js",
  "files": [
    "styles",
    "utils",
    "*.js",
    "!/*.config.js"
  ],
  "keywords": [
    "react",
    "react-component",
    "styled-components",
    "ui"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "prop-types": "^15.0.0",
    "styled-components": "^3.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "polished": "^1.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-external-helpers": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.56",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.56",
    "@babel/plugin-proposal-do-expressions": "7.0.0-beta.56",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.56",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.56",
    "@babel/plugin-proposal-function-bind": "7.0.0-beta.56",
    "@babel/plugin-proposal-function-sent": "7.0.0-beta.56",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.56",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0-beta.56",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0-beta.56",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-beta.56",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.56",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0-beta.56",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.56",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.56",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.56",
    "@babel/plugin-transform-object-assign": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-flow": "^7.0.0-beta.56",
    "@babel/preset-react": "^7.0.0-beta.56",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-runtime": "^6.26.0",
    "bundlesize": "^0.17.0",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.78.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-enzyme": "^6.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.0.0-beta.8",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-visualizer": "^0.8.1",
    "styled-components": "^3.4.2",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-styled-components": "^1.3.2"
  },
  "scripts": {
    "build": "npm run clean && npm run build-packages && npm run build-bundle",
    "build-bundle": "cross-env NODE_ENV=production rollup -c",
    "build-packages": "cross-env NODE_ENV=production babel --plugins '@babel/transform-modules-commonjs' src --out-dir . --copy-files",
    "build-storybook": "build-storybook -o docs",
    "watch": "cross-env NODE_ENV=production rollup -cw",
    "clean": "del '*.js' '!*.config.js' styles utils",
    "lint": "eslint --ext .js --ext .jsx src",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "bundlesize": "bundlesize",
    "precommit": "npm run lint && npm run build && bundlesize",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 9001"
  },
  "bundlesize": [
    {
      "path": "./index.es.js",
      "maxSize": "15 kB"
    },
    {
      "path": "./index.js",
      "maxSize": "15 kB"
    }
  ]
}
